<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rkswl.ezen_project">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".fragment.FragmentFindPassword" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".fragment.FragmentSettingPassword" />
        <activity android:name=".activity.SplashActivity" />
        <activity android:name=".fragment.FragmentJoinViewPager" />
        <activity android:name=".activity.SettingActivity" />
        <activity android:name=".ViewPagerAdapter.MainViewPagerAdapter" />
        <activity android:name=".ViewPagerAdapter.StoryViewPagerAdapter" />
        <activity android:name=".fragment.FragmentStory" />
        <activity android:name=".fragment.FragmentPicture" />
        <activity android:name=".fragment.FragmentStoryRecyclerView" />
        <activity android:name=".fragment.FragmentPictureRecyclerView" />
        <activity android:name=".fragment.FragmentStoryList" />
        <activity android:name=".fragment.FragmentStoryListRecyclerView" />
        <activity android:name=".fragment.FragmentStoryView" />
        <activity android:name=".fragment.FragmentPictureView" />
        <activity android:name=".activity.PlusActivity"></activity>
    </application>

</manifest>